{
    "name": "laravel-enso/localisation",
    "description": "Localisation management dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "localisation"
    ],
    "homepage": "https://github.com/laravel-enso/Localisation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel-enso/core": "4.0.*",
        "laravel-enso/formbuilder": "3.0.*",
        "laravel-enso/helpers": "1.10.*",
        "jenssegers/date": "^3.4",
        "laravel-enso/select": "3.0.*",
        "laravel-enso/structuremanager": "3.0.*",
        "laravel-enso/vuedatatable": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Localisation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Localisation\\AppServiceProvider",
                "LaravelEnso\\Localisation\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}
